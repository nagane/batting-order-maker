%html
  %head
    %script{:type => 'text/javascript', :src => "./js/jquery-2.1.4.min.js"}
  %body
    %h1 SHOW USERS DATA
    %a{ href: "/" } home
    %table
      - x = 0
      %tr
        %td
          %p name
        - @users_data.each do |user| 
          %td
            %p= user.name
      %tr
        %td 
          %p 1
        - @users_data.each do |user| 
          %td
            %p= user[1]
